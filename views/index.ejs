 <main class="website">

  <!-- Hero Section -->
  <section class="hero" aria-labelledby="welcome-title">
    <div class="hero-inner">
      <h1 id="welcome-title" class="hero-title">Welcome to CSE Motors!</h1>
        <!-- <%- messages %> -->
      <div class="hero-content">
        <div class="hero-copy">
          <h2 class="vehicle-title">DMC Delorean</h2>
          <ul class="vehicle-features">
            <li>3 Cup holders</li>
            <li>Superman doors</li>
            <li>Fuzzy dice!</li>
          </ul>
          <a class="btn btn-primary" href="#" aria-label="Own the DMC Delorean today">Own Today</a>
        </div>

        <figure class="hero-image">
          <img src="/images/vehicles/delorean-tn.jpg" alt="Illustration of the DMC Delorean">
        </figure>
      </div>
    </div>
  </section>

  <!-- Reviews Section -->
  <section class="reviews" aria-labelledby="reviews-title">
    <div class="section-inner">
      <h2 id="reviews-title">DMC Delorean Reviews</h2>
      <ul class="review-list">
        <li class="review-item">
          <blockquote>
            <p>“So fast it’s almost like traveling in time.”</p>
            <footer aria-label="Rating 4 out of 5">
              <span class="rating" aria-hidden="true">★★★★☆</span>
              <span class="sr-only">4/5</span>
            </footer>
          </blockquote>
        </li>
        <li class="review-item">
          <blockquote>
            <p>“Coolest ride on the road.”</p>
            <footer aria-label="Rating 4 out of 5">
              <span class="rating" aria-hidden="true">★★★★☆</span>
              <span class="sr-only">4/5</span>
            </footer>
          </blockquote>
        </li>
        <li class="review-item">
          <blockquote>
            <p>“I’m feeling McFly!”</p>
            <footer aria-label="Rating 5 out of 5">
              <span class="rating" aria-hidden="true">★★★★★</span>
              <span class="sr-only">5/5</span>
            </footer>
          </blockquote>
        </li>
        <li class="review-item">
          <blockquote>
            <p>“The most futuristic ride of our day.”</p>
            <footer aria-label="Rating 4.5 out of 5">
              <span class="rating" aria-hidden="true">★★★★½</span>
              <span class="sr-only">4.5/5</span>
            </footer>
          </blockquote>
        </li>
        <li class="review-item">
          <blockquote>
            <p>“80’s livin and I love it!”</p>
            <footer aria-label="Rating 5 out of 5">
              <span class="rating" aria-hidden="true">★★★★★</span>
              <span class="sr-only">5/5</span>
            </footer>
          </blockquote>
        </li>
      </ul>
    </div>
  </section>

  <!-- Upgrades Section -->
  <section class="upgrades" aria-labelledby="upgrades-title">
    <div class="section-inner">
      <h2 id="upgrades-title">Delorean Upgrades</h2>
      <div class="upgrade-grid">
        <a class="upgrade-card" href="#" aria-label="Flux Capacitor upgrade">
          <img src="/images/upgrades/flux-cap.png" alt="Flux Capacitor icon">
          <span class="upgrade-label">Flux Capacitor</span>
        </a>
        <a class="upgrade-card" href="#" aria-label="Hub Cap upgrade">
          <img src="/images/upgrades/hub-cap.jpg" alt="Hub Cap icon">
          <span class="upgrade-label">Hub Cap</span>
        </a>
        <a class="upgrade-card" href="#" aria-label="Bumper Sticker upgrade">
          <img src="/images/upgrades/bumper_sticker.jpg" alt="Bumper Sticker icon">
          <span class="upgrade-label">Bumper Sticker</span>
        </a>
        <a class="upgrade-card" href="#" aria-label="Flame Decals upgrade">
          <img src="/images/upgrades/flame.jpg" alt="Flame Decals icon">
          <span class="upgrade-label">Flame Decals</span>
        </a>
      </div>
    </div>
  </section>

</main>